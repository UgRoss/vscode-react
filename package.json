{
  "name": "vscode-react-snippets",
  "displayName": "React Snippets",
  "description": "ReactJS, Redux and React Router code snippets with hooks support.",
  "version": "2.0.1",
  "publisher": "ugross",
  "icon": "images/icon.png",
  "galleryBanner": {
    "theme": "light"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/UgRoss/vscode-react"
  },
  "engines": {
    "vscode": "^1.19.0",
    "node": ">=16.13.0"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "react",
    "redux",
    "snippets",
    "es6",
    "es7"
  ],
  "author": "Rostyslav Ugryniuk <contact@ugross.dev>",
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./build/snippets/javascript.json"
      },
      {
        "language": "javascriptreact",
        "path": "./build/snippets/javascript.json"
      },
      {
        "language": "typescript",
        "path": "./build/snippets/typescript.json"
      },
      {
        "language": "typescriptreact",
        "path": "./build/snippets/typescript.json"
      }
    ]
  },
  "scripts": {
    "compile": "tsc",
    "watch": "tsc -watch",
    "build": "npm run compile && node build/generateSnippetsPaths.js",
    "vscode:prepublish": "npm run build"
  },
  "ts-standard": {
    "project": "./tsconfig.json"
  },
  "dependencies": {
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@types/rimraf": "^3.0.2",
    "prettier": "^2.6.1",
    "ts-standard": "^11.0.0",
    "typescript": "^4.6.3",
    "vscode": "^1.1.37"
  }
}
